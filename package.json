    "body-parser": "^1.19.2",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.6",
    "promisify": "0.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
